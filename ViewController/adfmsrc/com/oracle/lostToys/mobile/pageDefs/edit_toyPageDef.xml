      <NamedData NDName="uuid" NDValue="#{bindings.addNewToy_uuid}" NDType="java.lang.String"/>
      <NamedData NDName="major" NDValue="#{bindings.addNewToy_major}" NDType="int"/>
      <NamedData NDName="minor" NDValue="#{bindings.addNewToy_minor}" NDType="int"/>
